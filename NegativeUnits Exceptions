public class NegativeUnitonHand extends Exception
{
   public NegativeUnitonHand()
   { 
   super(" Error: Negative units on hand");
   }
   public NegativeUnitonHand( int unitsOnHand)
   {
   super("Error: Negative units on hand amount "+ unitsOnHand);
   }
 }
